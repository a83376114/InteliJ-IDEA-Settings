<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="FONT_FAMILY" value="Noto Sans" />
    <option name="LINE_SPACING" value="1.2" />
    <option name="SECONDARY_FONT_FAMILY" value="Droid Sans Mono" />
  </component>
  <component name="LiveEditOptions" chromeAutoDelay="30" chromeUpdateJSOnChanges="true" restartIfHotSwapFailed="true" useJBChromeExtension="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>